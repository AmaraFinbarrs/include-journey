<div class="row justify-content-end mb-3">
  <div class="col">
    <div class="input-group limit-select">
      <select class="form-select" aria-label="Index Limit">
        <option value="<%= multiple %>" selected><%= multiple %></option>
        <option value="<%= multiple * 2 %>"><%= multiple* 2 %></option>
        <option value="<%= multiple * 5 %>"><%= multiple * 5 %></option>
        <option value="<%= multiple * 10 %>"><%= multiple * 10 %></option>
      </select>
    </div>
  </div>
</div>

<%= javascript_pack_tag 'limit' %>
