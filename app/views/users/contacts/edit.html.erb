<main class="container user contacts new">
  <%= render 'shared/back_btn' %>
  <section class="row">
    <div class="col">
      <h3>Edit Contact</h3>
      <%= render 'new_edit_form', submit_btn_text: 'Update' %>
      <p> <%= button_to "Remove", contact_path(@contact), data: { confirm: "Are you sure?" }, method: :delete, class: "btn btn-danger" %></p>
    </div>
  </section>
</main>